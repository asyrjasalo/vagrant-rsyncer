{
  "paths": [
    {
      "verbose": true,
      "source": {
        "path": "files",
        "excludes": [
          "excluded_file.txt",
          "directory/excluded_directory",
          "*glob_excluded_file*",
          "*glob_excluded_directory"
        ],
        "initial": {},
        "listen": {
          "latency": 0.1
        }
      },
      "target": {
        "path": "/home/vagrant/files",
        "user": "vagrant",
        "group": "vagrant",
        "permissions": {
          "user": "rwX",
          "group": "rX",
          "other": ""
        },
        "args": {
          "rsync": ["-a", "--delete", "--delete-excluded", "--force", "--inplace", "--out-format=Rsynced: %n%L"],
          "ssh": ["-o StrictHostKeyChecking=no", "-o UserKnownHostsFile=/dev/null"]
        }
      }
    }
  ]
}
