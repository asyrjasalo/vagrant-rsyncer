{
  "verbose": false,
  "args": {
    "rsync": ["-a", "--delete", "--delete-excluded", "--force", "--inplace"],
    "ssh": ["-o StrictHostKeyChecking=no", "-o UserKnownHostsFile=/dev/null", "-c arcfour"]
  },
  "paths": [
    {
      "source": {
        "watch": true,
        "latency": 0.1,
        "path": "files",
        "excludes": [
          "excluded_file.txt",
          "directory/excluded_directory",
          "*glob_excluded_file*",
          "*glob_excluded_directory"
        ]
      },
      "target": {
        "path": "/home/vagrant/files",
        "user": "vagrant",
        "group": "vagrant",
        "permissions": {
          "user": "rwX",
          "group": "rX",
          "other": ""
        }
      }
    }
  ]
}
