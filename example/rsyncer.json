{
  "paths": {
    "source": "files/",
    "target": "/home/vagrant/files"
  },
  "latency": 0.1,
  "verbose": false,
  "target": {
    "user": "vagrant",
    "group": "vagrant",
    "permissions": ["--chmod=u=rwX", "--chmod=g=rX", "--chmod=o="]
  },
  "args": {
    "rsync": ["-a", "--delete", "--force", "--inplace"]
  },
  "excludes": [
    "excluded_file.txt",
    "directory/excluded_directory",
    "glob_excluded_file*"
  ]
}
